
<!DOCTYPE html>
<html>
<head>
  <title><%= yield(:title)%> | Bedriftslotteri</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <%= javascript_include_tag "https://js.stripe.com/v2/" %>
  <%= javascript_include_tag "https://stripe.github.io/jquery.payment/lib/jquery.payment.js" %>
  <%= favicon_link_tag 'favicon.ico' %>
  <%= tag :meta, :name => "stripe-key", :content => 'pk_test_DoMFtdJPz48E2H26940WhSra' %>
  <%= tag 'meta', {:content => "width=device-width, initial-scale=1.0", :name => "viewport"}, true %>

</head>
<body>
<%= render "layouts/header" %>
<div class="container">
  <%= render "layouts/flash_messages" %>
</div>
<%= yield %>

</body>
</html>
